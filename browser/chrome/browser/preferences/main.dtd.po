#: startup.label
msgid "Startup"
msgstr "הפעלה"

#: startupPage.label
#: startupPage.accesskey
msgid "When &brandShortName; &starts:"
msgstr "כאשר &brandShortName; &מופעל:"

#: startupHomePage.label
msgid "Show my home page"
msgstr "הצג את דף הבית שלי"

#: startupBlankPage.label
msgid "Show a blank page"
msgstr "הצג דף ריק"

#: startupLastSession.label
msgid "Show my windows and tabs from last time"
msgstr "הצג את החלונות והלשוניות שלי מהפעם האחרונה"

#: homepage.label
#: homepage.accesskey
msgid "Home &Page:"
msgstr "&דף בית:"

#: useCurrentPage.label
#: useCurrentPage.accesskey
msgid "Use &Current Page"
msgstr "השתמש בדף ה&נוכחי"

#: useMultiple.label
msgid "Use Current Pages"
msgstr "השתמש בדפים הנוכחיים"

#: chooseBookmark.label
#: chooseBookmark.accesskey
msgid "Use &Bookmark…"
msgstr "השתמש ב&סימנייה…"

#: restoreDefault.label
#: restoreDefault.accesskey
msgid "&Restore to Default"
msgstr "&שחזר לברירת המחדל"

#: downloads.label
msgid "Downloads"
msgstr "הורדות"

#: saveTo.label
#: saveTo.accesskey
msgid "Sa&ve files to"
msgstr "שמור &קבצים אל"

#: chooseFolderWin.label
#: chooseFolderWin.accesskey
msgid "Br&owse…"
msgstr "ע&יון…"

#: chooseFolderMac.label
#: chooseFolderMac.accesskey
msgid "Choos&e…"
msgstr "&בחירה…"

#: alwaysAsk.label
#: alwaysAsk.accesskey
msgid "&Always ask me where to save files"
msgstr "שאל תמיד &היכן לשמור קבצים"

#: alwaysCheckDefault2.label
#: alwaysCheckDefault2.accesskey
msgid "Al&ways check if &brandShortName; is your default browser"
msgstr "&תמיד לבדוק אם &brandShortName; הוא דפדפן בררת מחדל"

#: setAsMyDefaultBrowser2.label
#: setAsMyDefaultBrowser2.accesskey
msgid "Make &Default"
msgstr "להגדיר כדפדפן בררת &מחדל"

#: isDefault.label
msgid "&brandShortName; is currently your default browser"
msgstr "&brandShortName; הוא דפדפן בררת המחדל שלך"

#: isNotDefault.label
msgid "&brandShortName; is not your default browser"
msgstr "&brandShortName; אינו דפדפן בררת המחדל שלך"

#: separateProfileMode.label
msgid "Allow &brandShortName; and Firefox to run at the same time"
msgstr "לאפשר ל־&brandShortName; ול־Firefox לרוץ במקביל"

#: useFirefoxSync.label
msgid "Tip: This uses separate profiles. Use Sync to share data between them."
msgstr ""
"טיפ: פעולה זו מתאפשרת תודות ליצירה של שני פרופילים נפרדים. ניתן להשתמש ב־"
"Sync כדי לסנכרן נתונים בניהם."

#: getStarted.label
msgid "Start using Sync…"
msgstr "התחלת שימוש ב־Sync…"
