msgctxt "noprinter"
msgid "No printers available."
msgstr "אין מדפסות זמינות."

#: PrintToFile
msgid "Print To File"
msgstr "הדפס לקובץ"

#: noPrintFilename.title
msgid "Filename is missing"
msgstr "שם קובץ חסר"

#: noPrintFilename.alert
